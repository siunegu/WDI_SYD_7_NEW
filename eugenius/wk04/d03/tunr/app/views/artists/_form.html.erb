<%= form_for @artist do |f| %>
	<%= f.label :name %>
	<%= f.text_field :name, :placeholder => 'Wolfgang Gartner' %>

	<%= f.label :label %>	
	<%= f.text_field :label, :placeholder => 'Spinning Records' %>

	<%= f.label :location %>	
	<%= f.text_field :location, :placeholder => 'United States' %>

	<%= f.submit %>
<% end %>